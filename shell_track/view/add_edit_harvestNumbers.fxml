<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="218.0" prefWidth="406.0" style="-fx-background-color: white; -fx-border-color: #155c72; -fx-border-width: 5;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shell_track.view.HarvesterNumberController">
    <children>
        <Label layoutX="201.0" layoutY="69.0" prefHeight="18.0" prefWidth="176.0" text="Original Harvest Number:" textFill="#155c72">
         <font>
            <Font name="Book Antiqua Bold Italic" size="14.0" />
         </font></Label>
        <TextField fx:id="newHarvesterNumber" alignment="CENTER" layoutX="178.0" layoutY="87.0" prefHeight="27.0" prefWidth="201.0" promptText="Harvest Number">
         <font>
            <Font name="Book Antiqua Bold" size="12.0" />
         </font></TextField>
        <Button fx:id="addHarvestNumber" layoutX="200.0" layoutY="122.0" mnemonicParsing="false" onAction="#addHarvesterNumber" prefHeight="27.0" prefWidth="57.0" text="Add">
         <font>
            <Font name="Book Antiqua Bold Italic" size="14.0" />
         </font></Button>
        <Button fx:id="cancelAdd" layoutX="236.0" layoutY="158.0" mnemonicParsing="false" onAction="#cancelAdd" prefHeight="28.0" prefWidth="79.0" text="Cancel">
         <font>
            <Font name="Book Antiqua Bold Italic" size="14.0" />
         </font></Button>
        <Button fx:id="deleteHarvestNumber" layoutX="289.0" layoutY="122.0" mnemonicParsing="false" onAction="#deleteHarvesterNumber" prefHeight="27.0" prefWidth="65.0" text="Delete">
         <font>
            <Font name="Book Antiqua Bold Italic" size="14.0" />
         </font></Button>
        <SplitPane layoutX="7.0" layoutY="10.0" prefHeight="200.0" prefWidth="159.0">
            <items>
                <ListView fx:id="harvesterNumberList" prefHeight="168.0" prefWidth="129.0" style="-fx-background-color: white; -fx-border-color: #155c72; -fx-border-width: 5;" />
            </items>
        </SplitPane>
      <Label layoutX="169.0" layoutY="10.0" text="-Harvest Managment-" textFill="#155c72" underline="true">
         <font>
            <Font name="Book Antiqua Bold Italic" size="22.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
